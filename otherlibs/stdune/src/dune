(library
 (name stdune)
 (public_name stdune)
 (synopsis
  "Standard library of Dune.\nThis library offers no backwards compatibility guarantees. Use at your own risk.")
 (libraries
  unix
  csexp
  (re_export top-closure)
  (re_export ordering)
  (re_export dyn)
  (re_export pp)
  fs-io)
 (root_module root)
 (library_flags
  (:include flags/sexp))
 (foreign_stubs
  (language c)
  (names
   dune_flock
   readdir
   wait4_stubs
   platform_stubs
   copyfile_stubs
   signal_stubs)))
