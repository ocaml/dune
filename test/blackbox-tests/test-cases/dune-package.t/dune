(library
 (public_name a)
 (modules x))

(library
 (name c)
 (private_modules y)
 (public_name a.b.c)
 (modules y))

(library
 (name d)
 (public_name a.byte_only)
 (modes byte)
 (modules z))

(rule (with-stdout-to x.ml (echo "")))
(rule (with-stdout-to z.ml (echo "")))
(rule (with-stdout-to y.ml (echo "")))
(rule (with-stdout-to y.mli (echo "")))

(rule (with-stdout-to foo.txt (echo "")))

(install
 (section share)
 (package a)
 (files foo.txt)
)
